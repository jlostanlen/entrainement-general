<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="decath" package="com.axelor.decath.db"/>

  <entity name="Fournisseur" lang="java" >
    <string name="nom" title="Nom"/>
    <string name="email" title="Email"></string>
    <string name="telephone" title="Telephone"></string>
    <many-to-one ref="com.axelor.decath.db.Services" name="service" title="Service" />
    <many-to-one name="photo" title="Photo" ref="com.axelor.meta.db.MetaFile"/>
  </entity>
</domain-models>
